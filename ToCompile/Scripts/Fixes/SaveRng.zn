library saveRng requires RNG{
    integer seed;
    RNGenerator rng;
    function onInit() {
        rng = NewGenerator(GetRandomInt(0,10000000));
        TimerStart(CreateTimer(),1/200,true,function() {SetRandomSeed(GenerateInt(rng,0,1000000));});
        //TimerStart(CreateTimer(),1/200,true,function() {SetRandomSeed(0);});
    }
}